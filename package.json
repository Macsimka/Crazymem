{
  "name": "crazymem",
  "version": "1.0.0",
  "description": "Node.js bindings for crazymem",
  "main": "output/lib.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "npm run build && mocha -r ts-node/register test/lib.ts",
    "build": "tsc ./src/lib.ts --outDir ./output",
    "rebuild": "node-gyp build",
    "install": "npm run build && node-gyp configure && node-gyp build",
    "preinstall": "git submodule add git@github.com:rdbo/libmem.git ./libmem && git reset --hard a584c65"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "^8.0.3",
    "nan": "^2.14.1",
    "node-addon-api": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.18.21",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "mocha": "^8.1.3",
    "node-api-headers": "^0.0.1",
    "node-gyp": "9.0.0",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.7.4"
  },
  "gypfile": true
}
